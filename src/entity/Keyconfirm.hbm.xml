<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Keyconfirm" table="keyconfirm" schema="sagacloud">
        <id name="id">
            <column name="Id" sql-type="int(11)"/>
        </id>
        <property name="keyId">
            <column name="key_Id" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="hddId">
            <column name="hdd_Id" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="addDate">
            <column name="add_Date" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>